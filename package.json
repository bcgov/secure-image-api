{
  "name": "secimg-api",
  "version": "0.0.1",
  "description": "Secure Image API",
  "engine": "node 9.3.0",
  "engines": {
    "node": "9.3.0"
  },
  "main": "index.js",
  "dependencies": {
    "@bcgov/nodejs-common-utils": "^0.0.7",
