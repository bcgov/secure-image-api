    "minio": "^6.0.0",
    "multer": "^1.3.1",
    "nconf": "^0.10.0",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
